{
    "name": "venveo/craft-maintenancemode",
    "description": "Programmatically disable the website through command line",
    "type": "craft-plugin",
    "version": "1.0.3",
    "minimum-stability": "stable",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "maintenance"
    ],
    "support": {
        "docs": "https://github.com/venveo/craft-maintenancemode/blob/master/README.md",
        "issues": "https://github.com/venveo/craft-maintenancemode/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Venveo",
            "homepage": "https://dev.venveo.com"
        },
        {
            "name": "Ransom Roberson",
            "email": "ransom@venveo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "venveo\\maintenancemode\\": "src/"
        }
    },
    "extra": {
        "name": "Maintenance Mode",
        "handle": "maintenancemode",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/venveo/craft-maintenancemode/master/CHANGELOG.md",
        "class": "venveo\\maintenancemode\\MaintenanceMode",
        "developer": "Venveo",
        "developerUrl": "https://venveo.com"
    }
}
